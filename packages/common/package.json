{
	"name": "@workspace/common",
	"version": "1.0.0",
	"type": "module",
	"exports": {
		"./components/*": {
			"default": "./src/components/*.tsx"
		},
		"./hooks/*": {
			"default": "./src/hooks/*.ts"
		},
		"./lib/*": {
			"default": "./src/lib/*.ts"
		},
		"./styles/*": {
			"default": "./src/styles/*.css"
		},
		"./ui/*": {
			"default": "./src/ui/*.tsx"
		},
		"./utils/*": {
			"default": "./src/utils/*.ts"
		}
	},
	"dependencies": {
		"@base-ui/react": "^1.0.0",
		"@workspace/api": "workspace:*",
		"@workspace/config": "workspace:*",
		"cmdk": "^1.1.1",
		"next-themes": "^0.4.6",
		"sonner": "^2.0.7"
	}
}
