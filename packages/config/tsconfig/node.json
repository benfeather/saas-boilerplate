{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"lib": ["ESNext"],
		"module": "nodenext",
		"moduleDetection": "force",
		"moduleResolution": "nodenext",
		"noEmit": true,
		"resolveJsonModule": true
	}
}
